{
  "name": "bidding",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run watch-css\" \"vite\"",
    "build": "tsc -b && tailwindcss -i ./src/App.css -o ./dist/output.css && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "watch-css": "tailwindcss -i ./src/App.css -o ./dist/output.css --watch"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.8",
    "@microsoft/signalr": "^8.0.7",
    "axios": "^1.7.3",
    "jquery": "^3.7.1",
    "js-cookie": "^3.0.5",
    "popper.js": "^1.16.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@types/js-cookie": "^3.0.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.0",
    "vite": "^5.4.7"
  }
}
